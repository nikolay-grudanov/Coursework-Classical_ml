# Example .env for local developer. Copy to .env and edit values as needed.
# The project scripts will source .env if present and use these variables to locate conda.

# Full conda environment prefix (preferred). Example:
# CONDA_PREFIX=/home/mrr/miniconda3/envs/ml
CONDA_PREFIX=

# Alternatively, provide the conda environment name (e.g. ml)
CONDA_ENV_NAME=

# Path to conda "bin" directory containing 'activate' script. Optional fallback.
# Example: /home/mrr/miniconda3/bin
CONDA_BIN=

# Backwards compatibility: the original absolute prefix used on old machine
OLD_CONDA_PREFIX=/home/gna/anaconda3/envs/rocm
